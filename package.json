    "url": "https://github.com/PaulRBerg/PaulRBerg"
  },
  "bugs": {
    "url": "https://github.com/PaulRBerg/PaulRBerg/issues"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "bootstrap": "4.1.3",
    "devicon": "^2.0.0",
    "gh-pages": "^1.1.0",
    "jquery": "^3.2.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "popper.js": "^1.12.9",
    "react": "^16.0.0",
    "react-burger-menu": "^2.1.6",
    "react-dom": "^16.0.0",
    "react-icons": "^3.0.5",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-typist": "^2.0.4",
    "sanitize.css": "^6.0.0",
    "sass": "^1.0.0-beta.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/--watch --recursive",
    "start-js": "react-scripts start",
    "start": "NODE_PATH=src/ && npm-run-all -p watch-css start-js",
    "build": "NODE_PATH=src/ && npm run build-css && react-scripts build",
    "test": "NODE_PATH=src/ &&react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {}
}
